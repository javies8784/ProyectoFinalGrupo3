<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ProyectoG3.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ProyectoG3"
    xmlns:localVistas="clr-namespace:ProyectoG3.Vistas"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent         
         ContentTemplate="{DataTemplate localVistas:Vlogin}"
         Route="False"
        Shell.FlyoutBehavior="Disabled"
        
         />    
    
    <ShellContent
        ContentTemplate="{DataTemplate localVistas:Vprincipal}"
        Route="Vprincipal" />

    <ShellContent
        Title="listado de Multa"
        ContentTemplate="{DataTemplate localVistas:Vregistro}"
        Route="Vregistro" />

    <ShellContent
        Title="Registrar Multa"
        ContentTemplate="{DataTemplate localVistas:VregistroMultas}"
        Route="VregistroMultas" />

    <ShellContent
        Title="Tipos de Multa"
        ContentTemplate="{DataTemplate localVistas:VtipoMultas}"
        Route="VtipoMultas" />

</Shell>
